{
  "name": "@internet-of-people/examples",
  "version": "0.1.0-alpha.1",
  "author": {
    "url": "https://iop.global/dsf",
    "name": "Internet of People",
    "email": "prometheus@iop-ventures.com"
  },
  "description": "",
  "main": "./dist/main.js",
  "typings": "./dist/main.d.ts",
  "private": true,
  "scripts": {
    "build": "npm run build:src && npm run build:test",
    "build:src": "tsc -p . ",
    "build:test": "echo ignored || tsc -p tsconfig.test.json",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "tslint -p tsconfig.json -c ../../tslint.json --fix",
    "lint:test": "tslint -p tsconfig.test.json -c ../../tslint.json --fix",
    "clean": "rimraf dist && rimraf node_modules",
    "test": "jest --detectOpenHandles --no-cache --passWithNoTests",
    "example:transfer": "npm run build && node dist/main.js transfer",
    "example:before-proof": "npm run build && node dist/main.js before-proof"
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@arkecosystem/crypto": "^2.6.0-next.6",
    "@arkecosystem/core-interfaces": "^2.6.0-next.6",
    "@arkecosystem/client": "1.0.5",
    "@internet-of-people/did-manager": "0.1.0-alpha.1",
    "axios": "0.19.0",
    "inquirer": "7.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/inquirer": "6.5.0",
    "@types/axios": "0.14.0",
    "jest": "24.9.0",
    "jest-extended": "0.11.2",
    "rimraf": "3.0.0",
    "ts-jest": "24.1.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true
  }
}
